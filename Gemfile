source 'https://rubygems.org'

rails_version = '~> 3.2.6'

gem 'rails', rails_version

gem 'pg'
gem 'nokogiri'
gem 'slim'

gem 'slim'
gem 'sass-rails'
gem 'jquery-rails'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'rails-behaviors'
  gem 'bootstrap-sass', '~> 2.0.4.0'
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'therubyracer', group: :production
